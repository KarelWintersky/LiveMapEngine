{
  "title"         : "Winterland: WestFjord (with points of interest)",
  "type"          : "bitmap",
  "display": {
    "zoom": -2,
    "zoom_min": -3,
    "zoom_max": 2,
    "background_color"  : "transparent",
    "maxbounds": [[-0.5, -1], [1.5, 2]],
    "custom_css" : "style.css",
    "cursor": "pointer"
  },
  "display_defaults": {
    "empty": {
      "stroke": 0,
      "borderColor" : "#000000",
      "borderWidth" : 0,
      "borderOpacity": 0,
      "fill": 1,
      "fillColor": "#0000ff",
      "fillOpacity": 0.1
    },
    "present": {
      "stroke": 0,
      "borderColor" : "#000000",
      "borderWidth" : 0,
      "borderOpacity": 0,
      "fill": 1,
      "fillColor": "#00ff00",
      "fillOpacity": 0.1
    }
  },
  "image" : {
    "file"  : "west-poi.png",
    "width" : 7015,
    "height": 4960,
    "ox"    : 0,
    "oy"    : 0
  },
  "layout": {
    "file"   : "westfjord.svg",
    "layers" : [ "Paths" ]
  },
  "layers": {
    "Paths": {
      "hint" : "Все пути",
      "zoom_min" : -100,
      "zoom_max" : 100,
      "empty": {
        "stroke": 0,
        "borderColor" : "#000000",
        "borderWidth" : 0,
        "borderOpacity": 0,
        "fill": 1,
        "fillColor": "#0000ff",
        "fillOpacity": 0.1
      },
      "present": {
        "stroke": 0,
        "borderColor" : "#000000",
        "borderWidth" : 0,
        "borderOpacity": 0,
        "fill": 1,
        "fillColor": "#00ff00",
        "fillOpacity": 0.1
      }
    }
  }
}
