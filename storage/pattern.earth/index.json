{
  "title"         : "Map of Earth before Ascension",
  "type"          : "bitmap",
  "display": {
    "zoom": -2,
    "zoom_min": -3,
    "zoom_max": 2,
    "background_color"  : "#9FC4D9",
    "maxbounds": [[-0.5, -1], [1.5, 2]],
    "cursor": "pointer",
    "viewmode": "folio"
  },
  "display_defaults": {
    "empty": {
      "stroke": 0,
      "borderColor" : "#000000",
      "borderWidth" : 0,
      "borderOpacity": 0,
      "fill": 1,
      "fillColor": "#0000ff",
      "fillOpacity": 0.3
    },
    "present": {
      "stroke": 0,
      "borderColor" : "#000000",
      "borderWidth" : 0,
      "borderOpacity": 0,
      "fill": 1,
      "fillColor": "#00ff00",
      "fillOpacity": 0.4
    }
  },
  "image" : {
    "file"  : "before.png",
    "width" : 6600,
    "height": 5183,
    "ox"    : 0,
    "oy"    : 0
  },
  "layout": {
    "file"   : "before.svg",
    "layers" : [ "Paths" ]
  },
  "layers": {
    "Paths": {
      "hint" : "Все пути",
      "zoom" : -2,
      "zoom_min" : -100,
      "zoom_max" : 100,
      "empty": {
        "stroke": 0,
        "borderColor" : "#000000",
        "borderWidth" : 0,
        "borderOpacity": 0,
        "fill": 1,
        "fillColor": "#0000ff",
        "fillOpacity": 0.1
      },
      "present": {
        "stroke": 1,
        "borderColor" : "#000000",
        "borderWidth" : 1,
        "borderOpacity": 0,
        "fill": 0,
        "fillColor": "#00ff00",
        "fillOpacity": 0.1
      }
    }
  }
}
