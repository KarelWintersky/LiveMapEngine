{
  // название карты
  "title"         : "Map of Confederation",
  // тип карты
  "type"          : "vector | bitmap | tileset | wms",
  "display": {
    "zoom": 0,
    "zoom_min": -3,
    "zoom_max": 2,
    "zoom_mode": "smooth",
    "background_color"  : "transparent",
    "maxbounds": [[-0.5, -1], [1.5, 2]],
    "custom_css" : "style.css",
    "cursor": "pointer",
    "viewmode": "folio"
  },
  "display_defaults": {
    "empty": {
      "stroke": 0,
      "borderColor" : "#000000",
      "borderWidth" : 0,
      "borderOpacity": 0,
      "fill": 1,
      "fillColor": "#0000ff",
      "fillOpacity": 0.3
    },
    "present": {
      "stroke": 0,
      "borderColor" : "#000000",
      "borderWidth" : 0,
      "borderOpacity": 0,
      "fill": 1,
      "fillColor": "#00ff00",
      "fillOpacity": 0.4
    }
  },
  "source": {
    "type": "image | tiles | url",
    "file": "temp.svg",
    "path": "eso/{z}/{x}/{y}.jpg",
    "url": "",
    "zoom": 2,
    "zoom_min": 1,
    "zoom_max": 6
  },
  "image" : {
    "file"  : "temp.svg",
    "width" : 2104,
    "height": 1488,
    "ox"    : 0,
    "oy"    : 0
  },
  "layout": {
    "file"   : "temp.svg",
    "layers" : [ "Paths" ]
  },
  "layers": {
    "Paths": {
      "hint" : "Все пути",
      "zoom_min" : -100,
      "zoom_max" : 100,
      "empty": {
        "stroke": 0,
        "borderColor" : "#000000",
        "borderWidth" : 0,
        "borderOpacity": 0,
        "fill": 1,
        "fillColor": "#0000ff",
        "fillOpacity": 0.1
      },
      "present": {
        "stroke": 0,
        "borderColor" : "#000000",
        "borderWidth" : 0,
        "borderOpacity": 0,
        "fill": 1,
        "fillColor": "#00ff00",
        "fillOpacity": 0.1
      }
    }
  },
  "edit_templates": {
    "templates": [
      {
        "title"     : "Подробный шаблон",
        "description" : "Шаблон с филдсетами",
        "url"         : "tinymce.fieldsets.html"
      },
      {
        "title"       : "Краткий шаблон",
        "description" : "Шаблон plain/text",
        "url"         : "tinymce.plaintext.html"
      }
    ],
    "content_css" : "template-styles.css",
    "template_popup_width"  : 800,
    "template_popup_height" : 400
  },
  "colorbox": {
    "width" : "900px",
    "height": "700px"
  }
}
